{
  "name": "kirillemko/php-ews",
  "description": "Library for communicating with Exchange Web Services.",
  "homepage": "http://jamesarmes.com/php-ews/",
  "license": "MIT",
  "authors": [
    {
      "name": "Kirill Emelianenko",
      "email": "kirill.emko@mail.ru"
    }
  ],
  "prefer-stable": true,
  "require": {
    "php": ">=7.1.0",
    "kirillemko/php-ntlm": "*"
  },
  "require-dev": {
    "mustache/mustache": "~2.0",
    "squizlabs/php_codesniffer": "*",
    "phpmd/phpmd": "@stable",
    "phan/phan": "@stable"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "jamesiarmes\\PhpEws\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
